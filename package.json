{
  "name": "auto-trader",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/king-xorse",
  "author": "rkdwn <k0000000@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:clean": "rm -rf build",
    "build:compile": "tsc --build",
    "build:module": "cp -r node_modules build",
    "build": "run-s build:clean build:compile build:module",
    "config": "cp -av envs/develop.env .env",
    "start": "nodemon --exec ts-node -r tsconfig-paths/register --transpile-only src/main.ts",
    "start:commit": "printf '\n' >> .env && echo APP_VER=$npm_package_config_version >> .env && echo APP_COMMIT=`git rev-parse HEAD` >> .env && echo APP_DEPLOY_DATE=`git log -1 --pretty=format:\"%ci\"` >> .env",
    "start:daemon": "node build/main.js",
    "start:local": "run-s config start:commit start"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "cron": "^2.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.16",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "typedi": "^0.10.0",
    "typescript": "^4.6.4"
  }
}
